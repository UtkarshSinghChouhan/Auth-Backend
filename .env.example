# Database connection URI for MongoDB
MONGO_URI=<your_mongodb_connection_string>

# Secret key for signing JWT tokens
JWT_SECRET=<your_jwt_secret>

# Port on which the server should run (optional, default: 5000)
PORT=5000

#Local App url (e.g. http://localhost:3001) 
APPURL=<your_local_app_url>

# Optional: If you have any other configuration that varies per environment, add them here.
GOOGLE_CLIENT_ID=<your_google_client_id>
GOOGLE_CLIENT_SECRET=<your_google_client_secret>
GOOGLE_CALLBACK_URL=<your_google_callback_url>